{
  "name": "garden",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "nexus:check": "nexus build --entrypoint pages/api/graphql.ts",
    "db:start": "docker run --detach --publish 5432:5432 -e POSTGRES_PASSWORD=postgres --name nexus_prisma_garden postgres:10.12",
    "db:migrate": "prisma migrate save --experimental && prisma migrate up --experimental",
    "gen:prisma": "prisma generate",
    "build": "nexus build && next build",
    "start": "next start"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@prisma/client": "^2.4.1",
    "apollo-server-micro": "^2.16.1",
    "class-validator": "^0.12.2",
    "formik": "^2.1.5",
    "graphql": "^15.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-relay": "^0.6.0",
    "graphql-type-json": "^0.3.2",
    "next": "9.5.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.0.0-rc.3",
    "typedi": "^0.8.0",
    "typegraphql-prisma": "^0.4.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@prisma/cli": "^2.5.0",
    "@types/graphql-fields": "^1.3.3",
    "@types/graphql-relay": "^0.6.0",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "babel-plugin-transform-typescript-metadata": "^0.3.0",
    "concurrently": "^5.3.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
