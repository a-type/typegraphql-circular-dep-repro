{
  "name": "garden",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "db:start": "docker run --detach --publish 5432:5432 -e POSTGRES_PASSWORD=postgres --name nexus_prisma_garden postgres:10.12",
    "db:migrate": "prisma migrate save --experimental && prisma migrate up --experimental",
    "gen:prisma": "prisma generate",
    "nexus:reflection": "nexus dev --reflection --entrypoint pages/api/graphql.ts",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "formik": "^2.1.5",
    "next": "9.5.2",
    "nexus": "^0.26.1",
    "nexus-plugin-prisma": "^0.17.0",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "concurrently": "^5.3.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
